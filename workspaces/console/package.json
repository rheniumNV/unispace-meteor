{
  "name": "@unispace-meteor/console",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --require dotenv/config ./dist/server/index.js",
    "build": "run-p build:*",
    "build:res": "tsc --project ./tsconfig.res.json && node --require dotenv/config --enable-source-maps ./dist/res/index.js",
    "build:server": "tsc --project ./tsconfig.main.json",
    "dev": "run-p dev:*",
    "dev:res": "tsc-watch --project ./tsconfig.res.json --onSuccess \"node --require dotenv/config --enable-source-maps ./dist/res/index.js\"",
    "dev:server": "tsc-watch --project  ./tsconfig.main.json --onSuccess \"npm start\"",
    "clean": "node -e \"try{fs.rmSync('dist', { recursive: true, force: true });fs.rmSync('tsconfig.main.tsbuildinfo');fs.rmSync('tsconfig.res.tsbuildinfo');}catch(e){}\"",
    "test": "jest",
    "unitPackage:sync": "ts-node -P ./tsconfig.dev.json ./src/dev/unitPackage",
    "feedback:unit": "npm run feedback:fetch & npm run feedback:attachUnit",
    "feedback:fetch": "ts-node --require dotenv/config -P ./tsconfig.dev.json ./src/dev/resFeedback/fetch",
    "feedback:attachUnit": "ts-node --require dotenv/config  -P ./tsconfig.dev.json ./src/dev/resFeedback/attachUnit",
    "format": "prettier --write ./src/**/*.{ts,tsx,json}",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "brson.js": "^1.0.5"
  },
  "dependencies": {
    "@unispace-meteor/common": "^1.0.0",
    "@unispace-meteor/miragex": "^1.0.0",
    "@unispace-meteor/simulator": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jose": "^4.14.6",
    "json2emap": "^0.2.0",
    "react": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "uuid": "^9.0.0",
    "websocket": "^1.0.34",
    "ws": "^8.13.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
